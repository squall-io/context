{
  "name": "@squall.io/context",
  "version": "0.0.2",
  "main": "src/index",
  "license": "MIT",
  "keywords": [
    "dependency",
    "injection",
    "container",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/squall-io/context.git"
  },
  "scripts": {
    "test": "yarpm type-check && yarpm jest",
    "type-check": "yarpm tsc --project ./tsconfig.json --noEmit",
    "transpile": "yarpm babel src/ --delete-dir-on-start --source-maps --extensions .ts --out-dir dist",
    "dev:test": "nodemon --ext js,ts,json,lock --watch jest.config.js --watch tsconfig.json --watch src/ --watch spec/ --watch spec/.jest/ --exec 'yarpm run test'",
    "nodemon": "nodemon",
    "babel": "babel",
    "jest": "jest",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.3",
    "yarpm": "^1.2.0"
  }
}
